{
  "name": "@livefire2015/solid-ag-chat",
  "version": "1.0.3",
  "description": "SolidJS chat components with official AG-UI protocol support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:js": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "build": "npm run build:js",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solidjs",
    "chat",
    "ag-ui",
    "ai",
    "pydantic-ai",
    "components",
    "sse",
    "server-sent-events"
  ],
  "author": "livefire2015",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "babel-preset-solid": "^1.9.9",
    "solid-js": "^1.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-solid": "^2.10.0"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.39",
    "@ag-ui/core": "^0.0.39",
    "fast-json-patch": "^3.1.1",
    "rxjs": "^7.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livefire2015/solid-ag-chat.git"
  }
}
